{
  "private": true,
  "name": "@metafox/framework",
  "version": "5.0.1",
  "description": "metafox framework",
  "main": "src/index.ts",
  "keywords": [
    "metafox",
    "metafox/core",
    "metafox framework"
  ],
  "repository": {},
